{
    "Routes": [
        {
            "UpstreamHttpMethod": [ "POST", "GET", "PUT", "DELETE" ],
            "UpstreamPathTemplate": "/person/{everything}",
            "DownstreamPathTemplate": "/api/person/{everything}",
            "DownstreamScheme": "http",
            "DownstreamHostAndPorts": [
                {
                    "Host": "huytest",
                    "Port": 8080
                }
            ],
            "SwaggerKey": "person"
        },
        {
            "UpstreamHttpMethod": [ "POST", "GET", "PUT", "DELETE" ],
            "UpstreamPathTemplate": "/user/{everything}",
            "DownstreamPathTemplate": "/api/user/{everything}",
            "DownstreamScheme": "http",
            "DownstreamHostAndPorts": [
                {
                    "Host": "userservice",
                    "Port": 8080
                }
            ],
            "SwaggerKey": "user"
        },
        {
            "UpstreamHttpMethod": [ "POST", "GET", "PUT", "DELETE" ],
            "UpstreamPathTemplate": "/auth/{everything}",
            "DownstreamPathTemplate": "/api/auth/{everything}",
            "DownstreamScheme": "http",
            "DownstreamHostAndPorts": [
                {
                    "Host": "userservice",
                    "Port": 8080
                }
            ],
            "SwaggerKey": "auth"
        }
    ],
    "SwaggerEndPoints": [
        {
            "Key": "person",
            "Config": [
                {
                    "Name": "Person API V1",
                    "Version": "v1",
                    "Url": "http://huytest:8080/swagger/v1/swagger.json"
                }
            ]
        },
        {
            "Key": "user",
            "Config": [
                {
                    "Name": "User API V1",
                    "Version": "v1",
                    "Url": "http://userservice:8080/swagger/v1/swagger.json"
                }
            ]
        },
        {
            "Key": "auth",
            "Config": [
                {
                    "Name": "Auth API V1",
                    "Version": "v1",
                    "Url": "http://userservice:8080/swagger/v1/swagger.json"
                }
            ]
        }
    ],
    "GlobalConfiguration": {
        "BaseUrl": "https://localhost:5000"
    }
}