{
    "Routes": [
        {
            "UpstreamHttpMethod": [ "POST", "GET", "PUT", "DELETE" ],
            "UpstreamPathTemplate": "/user/{everything}",
            "DownstreamPathTemplate": "/api/user/{everything}",
            "DownstreamScheme": "http",
            "DownstreamHostAndPorts": [
                {
                    "Host": "userservice",
                    "Port": 8080
                }
            ],
